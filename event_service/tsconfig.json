{
    "compilerOptions": {
      "sourceMap": true,
      "strictNullChecks": true,
      "removeComments": true,
      "suppressImplicitAnyIndexErrors": true,
      "allowSyntheticDefaultImports": true,
      "strictPropertyInitialization": false,
      "jsx": "preserve",
      "allowJs": true,
      "baseUrl": ".",
      "paths": {
        "~/*": ["./*"],
        "@/*": ["./*"]
      }
    }
  }
  