{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "npm run dev",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev-debug"
            ],
            "port": 5858
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\start"
        },
        {
            "name": "NodeAttach",
            "type": "node",
            "request": "attach",
            "port": 5859,
            "address": "192.168.99.100",
            "restart": true,
            "sourceMaps": true,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "/usr/src/app/clipboard_site"
        },
        {
            "name": "LocalDebug",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "program": "${workspaceRoot}/node_modules/nuxt/bin/nuxt",
            "stopOnEntry": false,
            "args": ["dev"],
            "runtimeArgs": ["-r", "ts-node/register"],
            "internalConsoleOptions": "openOnSessionStart",
            "cwd": "${workspaceRoot}",
            "sourceMaps": true,
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "nuxt:*,app",
                "DUMMY_DATA": "1"
            }
          }
    ]
}