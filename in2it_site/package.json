    "nuxt-google-maps-module": "^1.6.0",
    "ts-loader": "^6.1.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "vue-badger-accordion": "^1.1.5",
    "vue-notification": "^1.3.16",
    "vuejs-datepicker": "^1.6.2",
    "vuejs-paginate": "^2.1.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
